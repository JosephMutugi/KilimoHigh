@{
    ViewBag.Title = "Home Page";
}
@{
    ViewBag.Title = "Bank Master";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ShowTopMenu = true;

}

<div class="row">
    <div class="panel-body">
        <form method="post" action="@Url.Action("SaveClassStream","Home")">

            <div class="col-md-12">
                <div class="col-md-12">

                    <table>

                        <tr>
                            <td>
                                Stream Name:
                            </td>
                            <td>
                                <input name="StreamName" class="text-box margin-top-bottom-2" style="width: 100%" />

                            </td>

                        </tr>

                    </table>


                </div>
            </div>

            <div class="col-md-12">
                <input type="submit" class="btn btn-primary bn-sm" value="Save" />

            </div>
        </form>
        <div>
            @{ 
                StudentRegisterEntities db = new StudentRegisterEntities();
                var data = db.ClassStreams.ToList();
            }
            @Html.Partial("~/Views/Home/ClassStreamsData.cshtml", data)
        </div>
    </div>
</div>