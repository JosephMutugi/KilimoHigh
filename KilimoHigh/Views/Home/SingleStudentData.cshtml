@{
    ViewBag.Title = "Home Page";
}
@{
    //ViewBag.Title = "Bank Master";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ShowTopMenu = true;

}

<div class="row">
    <div class="panel-body">
        <form method="post" action="@Url.Action("ShowStudentDataView","Home")">

            <div class="col-md-12">
                <div class="col-md-12">

                    <table>

                        <tr>
                            <td>
                                Student Id:
                            </td>
                            <td>
                                <input name="Id" class="text-box margin-top-bottom-2" style="width: 100%" />

                            </td>

                        </tr>

                    </table>


                </div>
            </div>

            <div class="col-md-12">
                <input type="submit" class="btn btn-primary bn-sm" value="Search" />

            </div>
        </form>
        <div>
            @{
                StudentRegisterEntities db = new StudentRegisterEntities();
                var data = db.Students.ToList();
            }
            @Html.Partial("~/Views/Home/ClassStudentData2.cshtml", data)
        </div>
    </div>
</div>